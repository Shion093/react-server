{
  "name": "react-server-alex",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "prod": "npm-run-all --parallel prod:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch --env.NODE_ENV=dev --progress",
    "prod:build-client": "webpack --config webpack.client.js --env.NODE_ENV=prod --env.production --progress"
  },
